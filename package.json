{
  "name": "ailo-wasm-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "compile-cpp": "em++ --bind src/lib/sieve.cpp -O1 -s WASM=1 -s ALLOW_MEMORY_GROWTH=1 -s 'ENVIRONMENT=\"web,worker\"' -s USE_ES6_IMPORT_META=0 -s EXPORT_ES6=1 -s MODULARIZE=1 -o src/wasm/sieve.js && cp src/wasm/sieve.wasm public/",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "4.0.3",
    "threads": "^1.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.5",
    "chart.js": "^3.9.1",
    "primeicons": "^6.0.1",
    "primereact": "^8.6.1",
    "threads-plugin": "^1.4.0"
  },
  "eslintIgnore": [
    "src/wasm/sieve.js"
  ]
}
